<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.MenuController" fx:id="menuPane">
    <Button fx:id="homeButton">
        <graphic>
            <ImageView>
                <image>
                    <Image url="/path/to/home_icon.png" />
                </image>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="dashboardButton">
        <graphic>
            <ImageView>
                <image>
                    <Image url="/path/to/dashboard_icon.png" />
                </image>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="settingsButton">
        <graphic>
            <ImageView>
                <image>
                    <Image url="/path/to/settings_icon.png" />
                </image>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="logoutButton">
        <graphic>
            <ImageView>
                <image>
                    <Image url="/path/to/logout_icon.png" />
                </image>
            </ImageView>
        </graphic>
    </Button>
</VBox>
